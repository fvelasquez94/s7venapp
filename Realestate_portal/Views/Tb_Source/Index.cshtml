@model IEnumerable<Realestate_portal.Models.Tb_Source>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout_editor.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Source_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Id_Company)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Source_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Id_Company)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id_Source }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id_Source }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id_Source })
        </td>
    </tr>
}

</table>
