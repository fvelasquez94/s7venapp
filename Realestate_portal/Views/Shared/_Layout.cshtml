<script>
    function CambiarBroker(broker) {
        var url = '@Url.Action("Dashboard","Portal", new { broker="IDCOMPANY"})';
        url = url.replace("IDCOMPANY", broker);
        window.location.href = url;
    }


</script>
@RenderSection("scripts", required: false);

